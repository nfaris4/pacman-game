{
    "tags": {
      "allowUnknownTags": true
    },
    "source": {
      "include": ["src/js"],
      "includePattern": ".js$",
      "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [],
    "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
    },
    "opts": {
      "destination": "./docs",
      "recurse": true,
      "verbose": true
    }
  }